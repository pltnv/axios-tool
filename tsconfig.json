{
  "compilerOptions": {
    "target": "es6",
    "module": "NodeNext",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "nodenext",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "declaration": true,
    "declarationMap": true,
    "noEmit": false,
    "outDir": "./build"
  },
  "include": ["index.ts"],
  "exclude": ["node_modules", "test.ts"]
}
